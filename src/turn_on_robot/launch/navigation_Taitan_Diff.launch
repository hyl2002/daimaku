
<launch>
 <!-- 开启机器人底层相关节点  -->
 <include file="$(find turn_on_robot)/launch/turn_on_robot_2022.launch" />
 <!-- 设置需要用于导航的地图  -->
 <arg name="map_file" default="$(find turn_on_robot)/map/starand_graph.yaml"/>
 <node name="map_server_for_test" pkg="map_server" type="map_server" args="$(arg map_file)">
 </node>

   <!-- 开启用于导航的自适应蒙特卡洛定位amcl_omni  运动方式是omni不是diff-->
   <!-- <include file="$(find turn_on_robot)/launch/include/amcl_omni.launch" /> -->
  <!-- 开启teb_local_planner导航算法-一般用于全向移动阿克曼等车-->
   <!-- <include file="$(find turn_on_robot)/launch/include/teb_local_planner.launch" />
   <include file="$(find turn_on_robot)/launch/include/laser_line_extraction.launch" /> -->
   
  <!-- <include file="$(find turn_on_robot)/launch/include/eband_local_planner.launch" /> -->
  <!-- <include file="$(find turn_on_robot)/launch/include/base_local_planner.launch" /> -->
 <!-- 开启用于导航的自适应蒙特卡洛定位amcl 运动方式是diff -->
   <include file="$(find turn_on_robot)/launch/include/amcl.launch" />
  <!-- 开启dwa_local_planner导航算法 一般用于差速车-->
  <include file="$(find turn_on_robot)/launch/include/dwa_local_planner.launch" />
   <node name='send_mark' pkg="turn_on_robot" type="send_mark.py">
  </node> 
</launch>




